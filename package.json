{
  "name": "bionode-fasta",
  "version": "0.1.0",
  "description": "FASTA parser for bionode",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter spec",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -R spec && rm -rf ./coverage",
    "coveralls": "istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls && rm -rf ./coverage",
    "build-browser": "webpack",
    "build-docs": "docco ./lib/bionode.js"
  },
  "keywords": [
    "bioinformatics",
    "fasta",
    "bionode"
  ],
  "author": "Alan Rice",
  "license": "MIT",
  "bin": {
    "bionode-fasta": "cli.js"
  },
  "dependencies": {
    "bionode": "bionode/bionode"
  },
  "devDependencies": {
    "mocha": "~1.17.0",
    "mocha-lcov-reporter": "0.0.1",
    "should": "~3.0.1",
    "istanbul": "~0.2.4",
    "coveralls": "~2.6.1",
    "gulp": "~3.4.0",
    "gulp-util": "~2.2.12",
    "gulp-livereload": "~0.2.0",
    "tiny-lr": "0.0.5",
    "webpack": "~1.3.1-beta8",
    "docco": "~0.6.2",
    "contributor": "^0.1.16"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/alanrice/bionode-fasta.git"
  }
}
