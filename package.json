  },
  "devDependencies": {
    "mocha": "~1.21.4",
    "mocha-lcov-reporter": "0.0.1",
    "should": "~4.0.4",
    "istanbul": "~0.3.0",
    "coveralls": "~2.6.1",
    "gulp": "~3.4.0",
    "gulp-util": "~2.2.12",
    "gulp-livereload": "~0.2.0",
    "tiny-lr": "0.0.5",
