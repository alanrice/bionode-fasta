    "concat-stream": "~1.4.6",
    "minimist": "~1.1.0",
    "browserify": "~5.9.3"
  },
  "devDependencies": {
    "mocha-lcov-reporter": "0.0.1",
    "istanbul": "~0.3.0",
    "coveralls": "~2.11.1",
    "gulp": "~3.8.7",
    "gulp-util": "~3.0.0",
    "gulp-livereload": "~2.1.0",
    "tiny-lr": "0.1.0",
    "webpack": "~1.3.3-beta1",
